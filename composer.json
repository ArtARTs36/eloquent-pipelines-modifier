{
    "name": "creatortsv/eloquent-pipelines-modifier",
    "type": "library",
    "license": "MIT",
    "description": "",
    "authors": [
        {
            "name": "creatortsv",
            "email": "creatortsv@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "creatortsv\\eloquent_pipelines_modifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": [
            "./vendor/bin/phpcs --standard=PSR2 src/",
            "./vendor/bin/phpcs --standard=PSR2 tests/"
        ],
        "test": [
            "./vendor/bin/phpunit -v --coverage-text --colors=always --configuration phpunit.xml"
        ]
    }
}
